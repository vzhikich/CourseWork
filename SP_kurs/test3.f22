PROGRAM Flyk22;
BEGIN
VAR INT16 a, b, i, x, j ;

SCAN (a);
SCAN (b);
i -> a;
WHILE ((i<<b)||(i==b));
DO
BEGIN
	PRINT(i * i);
	i-> i + 1;
END

x -> 0;
i -> 1; 

WHILE ((i << a)||(i==a));
DO
BEGIN
	j -> 1;
	WHILE((j << b)||(j==b));
	DO
	BEGIN
	x -> x + 1;
	j -> j + 1;
	END
i -> i + 1;
END

PRINT (x);
END
